<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AreaMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.pojo.Area">
        <id column="Ano" property="Ano" />
        <result column="Aname" property="Aname" />
        <result column="Aphoto" property="Aphoto" />
        <result column="Cno" property="Cno" />
        <result column="Astatus" property="Astatus" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        Ano, Aname, Aphoto, Cno, Astatus
    </sql>

</mapper>
